                {.end}
            {.end}
        {.end}
        <li class="navi__li navi__li--mobile">
            <div class="icon-wrap js-navi-link-mobile">
                <span class="icon icon--list">
                    <span class="top"></span>
                    <span class="mid"></span>
                    <span class="bot"></span>
                    <span class="ex1"></span>
                    <span class="ex2"></span>
                </span>
            </div>
        </li>
    </ul>
    <div class="navi__mobile js-navi-mobile screen screen--light ghost">
        <ul class="navi__mobile__ul ghost__child">
            {.repeated section items}
                {.section collection}
                    {.if homepage}
                    {.or}
                        <li class="navi__mobile__li navi__mobile__li--{title|slugify}">
                            <a class="navi__mobile__a navi__mobile__a--{title|slugify} js-navi-a js-navi--{title|slugify} {.if active}is-active{.end} {.if folderActive}is-active{.end}" href="{fullUrl}">{title}</a>
                        </li>
                    {.end}
                {.end}
            {.end}
        </ul>
    </div>
