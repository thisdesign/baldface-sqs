{# Sponsors detail page #}
{.equal? collection.id "599f468359cc68b53a3c74a1"}
    {.section item}
        <article class="article article--cover cover ghost -light js-cover">
            <div class="article__wrap ghost__child -full -exp-1">
                <div class="article__col -column -column--2of4">
                    <div class="article__info">
                        <div class="-cutoff">
                            <div class="article__logo">
                                <img class="image js-lazy-image" data-img-src="{assetUrl}" data-variants="{systemDataVariants}" />
                            </div>
                            <div class="h1 article__title">{title}</div>
                            <div class="p article__body">
                                {body}
                            </div>
                            <div class="article__button">
                                <a href="{clickthroughUrl}" class="button button--size-large button--naira button--naira-up button--border-medium button--inverted" target="_blank">
                                    <div class="button__hover">Launch Site</div>
                                    <div class="button__main">Launch Site</div>
                                </a>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="article__col -column -column--2of4">
                    <div class="article__image">
                        {.section customContent.extraImages}
                            {.repeated section @}
                                {@|apply image.block}
                            {.end}
                        {.end}
                    </div>
                </div>
            </div>
        </article>
    {.end}
{.end}
